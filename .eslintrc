{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"arrows-parens": "off",
		"coma-dangel": "off",
		"require-jsdoc": "off",
		"operator-linebreak": "off",
		"indent": "off",
		"eol-last": "off",
		"arrow-parens": "off",
		"no-tabs": "off"
	},
	"extends": ["eslint:recommended", "google"]
}